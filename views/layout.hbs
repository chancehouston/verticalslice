<!DOCTYPE html>
<html>
  <head>
    <title>{{title}}</title>
	<link rel="stylesheet" href="/css/style.css" media="only screen and (-webkit-min-device-pixel-ratio:0) and (min-color-index:0), (-ms-high-contrast: none), only all and (min--moz-device-pixel-ratio:0) and (min-resolution: 3e1dpcm)">
  </head>
  <body>
    {{{body}}}
	   {{!-- <script src="https://cdn.polyfill.io/v2/polyfill.min.js"></script> --}}
		<script type="module">
		import './javascript/bundle-es6-min.js';
	</script>
	<script>
		(function() {
    var linkEl = document.querySelector('link');
    if (window.matchMedia && window.matchMedia(linkEl.media).matches) {

		var polyfillscript = document.createElement('script');
		polyfillscript.src = "https://cdn.polyfill.io/v2/polyfill.min.js";
		document.body.appendChild(polyfillscript);

		var nomodulescript = document.createElement('script');
        nomodulescript.src = './javascript/bundle-es5-min.js';
		nomodulescript.setAttribute("nomodule", true);
        document.body.appendChild(nomodulescript);
    }
})();
	</script>
  </body>
</html>
