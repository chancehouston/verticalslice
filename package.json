{
  "name": "vertical-slice",
  "version": "0.0.0",
  "private": true,
  "scripts": {
	"up": "npm-run-all build start",
	"live": "livereload ./public/javascript ./public/css", 
	"build": "npm-run-all sass autoprefixer rollup transpilejs",
    "watch": "nodemon -e scss -x \"npm run build\"",
    "start": "node ./bin/www",
    "sass": "node-sass --output-style compressed -o public/css src/sass/style.scss --sourcemap",
    "autoprefixer": "postcss -u autoprefixer -r public/css/*",
    "rollup": "rollup --config --watch",
    "transpilejs": "./node_modules/google-closure-compiler-js/cmd.js  public/javascript/bundle-es6.js > public/javascript/bundle-es5.js"
  },
  "dependencies": {
    "@springernature/global-box": "^1.0.1",
    "@springernature/global-colors": "0.0.1",
    "@springernature/global-grid": "0.0.1",
    "@springernature/global-mq": "0.0.1",
    "autoprefixer": "^8.6.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "google-closure-compiler": "^20180506.0.0",
    "hbs": "~4.0.1",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "node-sass-import": "^2.0.1",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.0"
  },
  "devDependencies": {
    "@material/fab": "^0.36.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eyeglass": "^1.5.0",
    "google-closure-compiler-js": "^20180506.0.0",
    "livereload": "^0.7.0",
    "lodash": "^4.17.10",
    "lodash-es": "^4.17.10",
    "moment": "^2.22.2",
    "node-sass": "^4.9.0",
    "rollup": "^0.59.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-watch": "^4.3.1"
  }
}
